{
  "name": "leo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:table-to-json": "pnpm --filter './packages/table-to-json' run dev",
    "dev": "pnpm --filter './packages/**' run dev",
    "build": "pnpm --filter './packages/**' run build",
    "release": "pnpm run build; git add . && git commit -m \"feat: ðŸš€\" && git push -u origin main",
    "release:only": "changeset publish --tag=beta --access=publish",
    "format": "prettier --loglevel warn --write \"**/*.{js,jsx,ts,tsx,vue,css,md}\"",
    "posttest": "npm run format",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --cache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --cache --fix",
    "pretest": "npm run lint && tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "lodash": "^4.17.21",
    "semver": "^7.5.4",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.11.0",
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@types/jquery": "^3.5.22",
    "@types/semver": "^7.5.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vueuse/core": "^10.5.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "cssnano": "^6.1.2",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "execa": "^8.0.1",
    "husky": "^8.0.3",
    "inquirer": "^9.2.11",
    "lint-staged": "^15.0.1",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.2",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "sass-loader": "^13.3.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-monkey": "^3.5.0",
    "vue-tsc": "^1.8.19",
    "zx": "^7.2.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache --fix",
    "*.js": "eslint --cache --fix",
    "*.{ts,tsx,css,md}": "prettier --write"
  }
}
